<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidfitness">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidFitness2">
        <activity
            android:name=".userinterface.Locations_Activity"
            android:exported="false"
            android:label="Locations"
            android:parentActivityName=".userinterface.MainActivity"/>
        <activity
            android:name=".userinterface.sleep.Sleep_UpdateActivity"
            android:exported="false"
            android:label="Update Sleep"
            android:parentActivityName=".userinterface.sleep.Sleep_Activity" />
        <activity
            android:name=".userinterface.sleep.Sleep_AddActivity"
            android:exported="false"
            android:label="Add Sleep"
            android:parentActivityName=".userinterface.sleep.Sleep_Activity" />
        <activity
            android:name=".userinterface.sleep.Sleep_Activity"
            android:exported="false"
            android:label="Sleep"
            android:parentActivityName=".userinterface.MainActivity" />
        <activity
            android:name=".userinterface.micronutrients.Micronutrients_UpdateActivity"
            android:exported="false"
            android:label="Update Vitamins"
            android:parentActivityName=".userinterface.micronutrients.Micronutrients_Activity" />
        <activity
            android:name=".userinterface.micronutrients.Micronutrients_AddActivity"
            android:exported="false"
            android:label="Add Vitamins"
            android:parentActivityName=".userinterface.micronutrients.Micronutrients_Activity" />
        <activity
            android:name=".userinterface.micronutrients.Micronutrients_Activity"
            android:exported="false"
            android:label="Micronutrients"
            android:parentActivityName=".userinterface.MainActivity" />
        <activity
            android:name=".userinterface.macronutrients.Macronutrients_UpdateActivity"
            android:exported="false"
            android:label="Update data"
            android:parentActivityName=".userinterface.macronutrients.Macronutrients_Activity" />
        <activity
            android:name=".userinterface.macronutrients.Macronutrients_AddActivity"
            android:exported="false"
            android:label="Add Macro Data"
            android:parentActivityName=".userinterface.macronutrients.Macronutrients_Activity" />
        <activity
            android:name=".userinterface.macronutrients.Macronutrients_Activity"
            android:exported="false"
            android:label="Macronutrient"
            android:parentActivityName=".userinterface.MainActivity" />
        <activity
            android:name=".userinterface.steps.Steps_UpdateActivity"
            android:exported="false"
            android:parentActivityName=".userinterface.steps.Steps_Activity" />
        <activity
            android:name=".userinterface.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".userinterface.steps.Steps_AddActivity"
            android:exported="false"
            android:label="Add Steps and Calories"
            android:parentActivityName=".userinterface.steps.Steps_Activity" />
        <activity
            android:name=".userinterface.steps.Steps_Activity"
            android:exported="false"
            android:label="Steps"
            android:parentActivityName=".userinterface.MainActivity" />
    </application>

</manifest>