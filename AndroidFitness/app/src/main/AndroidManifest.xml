<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidfitness2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidFitness2">
        <activity
            android:name=".UpdateActivity_Steps"
            android:exported="false"
            android:parentActivityName=".Activity_Steps"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddActivity_Steps"
            android:exported="false"
            android:label="Add Steps to recycler view"
            android:parentActivityName=".Activity_Steps" />
        <activity
            android:name=".Activity_Steps"
            android:exported="false"
            android:label="Steps"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>